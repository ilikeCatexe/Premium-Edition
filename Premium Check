local g1 = "https://raw.githubusercontent.com/ilikeCatexe/Premium-Edition/main/King%20Of%20The%20Hill"
local g2 = "https://raw.githubusercontent.com/ilikeCatexe/Premium-Edition/main/Marvellous%20Playground"

local gameid = game.PlaceId

print("GameId is", gameid)

wait(0)
if game.PlaceId == 3495989665 then
	loadstring(game:HttpGet(g1, true))() -- King Of The Hill
elseif game.PlaceId then
	loadstring(game:HttpGet(g2, true))() -- Marvellous Playground
end
